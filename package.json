{
    "name": "request-last",
    "version": "0.0.4",
    "description": "request-last",
    "main": "build/index.js",
    "scripts": {
        "start:build": "npm run tsc && node build",
        "start": "node run",
        "test": "jest --forceExit --detectOpenHandles",
        "clear": "rimraf build",
        "tsc": "npm run clear && tsc",
        "watch": "npm run build -- -w",
        "p": "npm run tsc && npm publish",
        "lint": "tslint \"src/**/*.ts\" --fix",
        "precommit": "npm run lint"
    },
    "dependencies": {"tslib": "^1.9.3"},
    "devDependencies": {
        "@types/node": "^12.0.2",
        "jest": "^25.1.0",
        "rimraf": "^2.6.3",
        "ts-node": "^8.1.0",
        "tslint": "^5.16.0",
        "typescript": "^3.4.5",
        "husky": "^4.2.3"
    },
    "keywords": [
        "typescript",
        "module-polierplate",
        "request-last"
    ],
    "husky": {
        "hooks": {"pre-commit": "npm run lint"}
    },
    "author": "hubiqing",
    "repository": "https://github.com/githbq/hbq-request-last.git",
    "engines": {"node": ">=7.6"},
    "license": "MIT"
}